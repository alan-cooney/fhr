{
    "manifest_version": 3,
    "name": "FH&R Helper",
    "version": "0.1.0",
    "description": "Don't share.",
    "content_scripts": [
        {
            "css": [
                "leaflet/leaflet.css",
                "styles.css"
            ],
            "js": [
                "leaflet/leaflet.js",
                "content.js"
            ],
            "matches": [
                "https://www.americanexpress.com/en-us/business/tls/partnerships/mystery-shop/Mystery/Map?program=FHR",
                "https://www.americanexpress.com/en-us/business/tls/partnerships/mystery-shop/Mystery/Map?program=THC"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "leaflet/images/marker-icon.png",
                "leaflet/images/marker-icon-2x.png"
            ],
            "matches": [
                "https://www.americanexpress.com/*"
            ]
        }
    ]
}
